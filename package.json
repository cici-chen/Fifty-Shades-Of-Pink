{
  "name": "fifty-shades-of-pink",
  "version": "1.0.0",
  "description": "A personal romance story generator",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cici-chen/Fifty-Shades-Of-Pink.git"
  },
  "main": "server/index.js",
  "scripts": {
    "dev": "nodemon --watch server server/index & webpack --watch",
    "start": "node 'server/index.js'",
    "test-jest": "jest --verbose tests/redux/*.test.js",
    "test:watch": "npm test-jest -- --watch",
    "test-ava": "ava -v --serial tests/**/*.test.js",
    "test-db": "ava -v --serial tests/db/*.test.js",
    "test-api": "ava -v --serial tests/api/*.test.js",
    "watch": "nodemon -e js,jsx -x npm test"
  },
  "ava": {
    "require": "babel-register",
    "babel": "inherit"
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ]
  },
  "author": "Cici-Chen",
  "license": "ISC",
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-core": "^6.21.0",
    "babel-jest": "^20.0.3",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.18.0",
    "enzyme": "^2.7.0",
    "jest": "^20.0.4",
    "jsdom": "^11.0.0",
    "nock": "^9.0.13",
    "nodemon": "^1.11.0",
    "react-addons-test-utils": "^15.5.1",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "react-test-renderer": "^15.5.4",
    "redux": "^3.6.0",
    "redux-mock-store": "^1.2.3",
    "redux-thunk": "^2.2.0",
    "sqlite3": "^3.1.8",
    "webpack": "^2.1.0-beta.21",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "es6-promise": "^4.1.0",
    "express": "^4.14.0",
    "isomorphic-fetch": "^2.2.1",
    "knex": "^0.13.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "superagent": "^3.5.2"
  }
}
